{
  "name": "@naturalcycles/semantic-release",
  "version": "1.3.0",
  "scripts": {
    "release": "node ./src/bin/release.js"
  },
  "dependencies": {
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "semantic-release": "^15.10.8",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.11",
