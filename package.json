{
  "name": "@naturalcycles/semantic-release",
  "version": "1.4.0",
  "scripts": {
    "release": "node ./src/bin/release.js"
  },
  "dependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "semantic-release": "^17.0.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/yargs": "^15.0.3"
  },
  "bin": {
